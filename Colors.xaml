<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#003366</Color>      <Color x:Key="Secondary">#F0F0F0</Color>    <Color x:Key="Tertiary">#C0A040</Color>     <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Black">#000000</Color>
    <Color x:Key="LightGray">#DDDDDD</Color>
    <Color x:Key="DarkGray">#505050</Color>
    
    <Color x:Key="Background">White</Color>
    <Color x:Key="Surface">White</Color>
